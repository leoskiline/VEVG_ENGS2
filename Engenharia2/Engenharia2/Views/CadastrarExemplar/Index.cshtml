@{
    ViewData["Title"] = "Cadastrar Exemplar";
}
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0">Pagina Cadastro de Exemplar</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="~/Home">Home</a></li>
                    <li class="breadcrumb-item active">Cadastro de Exemplar</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
<!-- Main content -->
<div class="content">
    <div class="container-fluid">
        <div class="">
            <form>
                <div class="form-group">
                    <label class="control-label col-sm-2" for="qtd">Quantidade:</label>
                    <div class="col-sm-2">
                        <input class="form-control" type="number" id="qtd" placeholder="Digite o a qtd">
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-2" for="setorPos">Setor:</label>
                    <div class="col-sm-2">
                        <input type="text" class="form-control" id="setorPos">
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-2" for="prateleiraPos">Prateleira:</label>
                    <div class="col-sm-2">
                        <input type="text" class="form-control" id="prateleiraPos">
                    </div>
                </div>
            </form>
        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</div>

@section javascript {
    <script src="~/js/views/exemplar/Index.js"></script>
    <script src="~/lib/HTTPClient.js"></script>
}